{
  "name": "discord-bot-ai-proxy",
  "main": "src/index.ts",
  "compatibility_date": "2026-02-20",
  "ai": {
    "binding": "AI",
  },
  "vars": {
    "CF_GATEWAY_ID": "discord-bot-ai",
    "DEFAULT_IMAGE_MODEL": "google/gemini-2.5-flash-image",
    "DEFAULT_CHAT_MODEL": "google/gemini-2.5-flash",
  },
  "ratelimits": [
    {
      "name": "IMAGE_RATE_LIMITER",
      "namespace_id": "1001",
      "simple": {
        "limit": 5,
        "period": 60,
      },
    },
    {
      "name": "CHAT_RATE_LIMITER",
      "namespace_id": "1002",
      "simple": {
        "limit": 15,
        "period": 60,
      },
    },
  ],
}
